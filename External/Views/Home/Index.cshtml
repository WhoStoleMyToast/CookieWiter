@{
    ViewBag.Title = "----------------------------- External -------------------------------------------";

    IList<HttpCookie> cookies = (List<HttpCookie>)ViewBag.Cookies;
}

<div class="jumbotron">
    <h1>EXTERNAL</h1>
</div>

<ul class="list-group">
    @foreach (HttpCookie cookie in cookies)
    {
        <li class="list-group-item">
            @cookie.Name
            <span class="badge">@cookie.Value</span>
        </li>
    }
</ul>